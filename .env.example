# Twitter CT0 token. See the README for instructions on how to obtain this.
TWITTER_CT0=your_ct0_token_here

# Twitter authentication token.
TWITTER_AUTH_TOKEN=your_auth_token_here

# Optional: Separate Twitter CT0 token for tweet scraping (if not provided, uses TWITTER_CT0).
TWITTER_SCRAPER_CT0=your_scraper_ct0_token_here

# Optional: Separate Twitter authentication token for tweet scraping (if not provided, uses TWITTER_AUTH_TOKEN).
TWITTER_SCRAPER_AUTH_TOKEN=your_scraper_auth_token_here

# Base directory for all MindVault data.
BASE_DIR=your_base_dir_here

# Path to the bookmarks JSON file.
BOOKMARKS_PATH=your_bookmarks_path_here

# Path to the file that stores tweet IDs.
TWEET_IDS_PATH=your_tweet_ids_path_here

# Directory where tweet data will be stored.
TWEET_DATA_DIR=your_tweet_data_dir_here

# Directory where extracted tweet data will be stored.
EXTRACTED_DATA_DIR=your_extracted_data_dir_here

# Database connection URL (e.g., sqlite:////path/to/database.db).
DATABASE_URL=your_database_url_here

# Path to the JSON file with pending tweets.
PENDING_TWEETS_PATH=your_pending_tweets_path_here
